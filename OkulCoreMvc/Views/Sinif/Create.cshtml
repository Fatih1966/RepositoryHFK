@model Okul.Entity.Models.Sinif



<form asp-action="Create" method="post" class="form-group">



    <div class="form-group">
        <div class="row">
            <div class="col-md-4">
                <label asp-for="@Model.SinifAdi" class="form-control"></label>
            </div>
            <div class="col md-8">
                <input asp-for="@Model.SinifAdi" class="form-control" />
                <span asp-validation-for="@Model.SinifAdi" class="text-danger"></span>
            </div>
        </div>
    </div>

    <div class="form-group">
        <div class="row">
            <input type="submit" value="Ekle" class="btn btn-success" />
            <a asp-action="Index" asp-controller="Brans" class="btn btn-primary">Geri Don</a>

        </div>
    </div>
</form>